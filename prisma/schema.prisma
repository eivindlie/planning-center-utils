generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url = env("DATABASE_URL")
}

model Team {
    id      String  @default(cuid())    @id
    name    String
    members Member[]
}

model Member {
    id      String  @default(cuid())    @id
    pcId    String
    name    String
    team    Team?   @relation(fields: [teamId], references: [id])
    teamId  String?
}